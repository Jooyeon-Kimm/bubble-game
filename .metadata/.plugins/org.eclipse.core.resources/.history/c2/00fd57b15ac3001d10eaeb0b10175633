package bubble.test.ex02;

import javax.swing.JFrame;

public class BubbleFrame extends JFrame {
	
	public BubbleFrame() {
		setSize(1000, 640);
		setLayout(null); //absoulte 레이아웃 (자유롭게)
		setLocationRelativeTo(null); // 창이 가운데 뜨도록
		setVisible(true);
	}
	
	public static void main(String[] args) {
		new BubbleFrame();
	}

}
